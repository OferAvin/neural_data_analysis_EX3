function [pass] = is_valid_block(has_passed,min_correct_ans)
    has_passed = filter_limit_time(rt,max_lim,min_lim,has_passed);
    has_passed = filter_num_of_wrong_ans(acc,min_correct_ans,has_passed);
    valid_ans = find(has_passed == 'true');
    pass = true;
    if length(valid_ans) < min_correct_ans
        pass = false;
    end
end

    